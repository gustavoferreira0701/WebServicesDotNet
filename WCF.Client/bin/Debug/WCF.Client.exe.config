<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMensagem" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IMensagem" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:9090/Mensagem" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IMensagem" contract="Servico.IMensagem"
                name="NetTcpBinding_IMensagem">
                <identity>
                    <userPrincipalName value="XPS-GUSTAVO\Gustavo" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:9092/Mensagem" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMensagem" contract="Servico.IMensagem"
                name="BasicHttpBinding_IMensagem" />
        </client>
    </system.serviceModel>
</configuration>